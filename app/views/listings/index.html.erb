<div class="jumbotron">
  <h1>To find your dream house in Ireland.</h1>
  <h2>Have a look and enjoy yourself.</h2>
</div>

<div class="center">
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-md-3">
        <div class="thumbnail">
          <%= link_to image_tag(listing.image.url), listing %>
          <div class="caption">
            <h3><%= listing.city %></h3>
            <p>Deposit:  € <%= listing.deposit %></p>
            <p>Rental: € <%= listing.rental %></p>
            <p>Address: <%= listing.address %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

  <div class="text-center">
    <%= will_paginate %>
  </div>

